<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

  <Button
      android:id="@+id/btn_register_str"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="register for str" />

  <Button
      android:id="@+id/btn_register_publishable"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="register publishable" />

  <Button
      android:id="@+id/btn_unregister"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="unregister" />

  <Button
      android:id="@+id/btn_debug_dump_cached"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="debug dump cached" />

</LinearLayout>
